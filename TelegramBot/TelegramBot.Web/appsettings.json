{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "AllowedHosts": "*",

  "TelegramBot": {
    "Token": "api key"
  },

  "ConnectionStrings": {
    "DefaultConnection": "Server=mssql.chatbot-dev.svc.cluster.local,1433;Database=TelegramBot;User Id=sa;Password=Personal1;TrustServerCertificate=True;"
  },

  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ],
    "Properties": {
      "Application": "TelegramBot"
    }
  },

  "Tokens": {
    "Cohere": "",
    "Telegram": ""
  }
}
